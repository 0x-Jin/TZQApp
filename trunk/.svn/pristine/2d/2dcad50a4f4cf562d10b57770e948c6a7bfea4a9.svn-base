
<div style="padding-top:15px;margin-bottom: 35px; padding-left: 10%;padding-right: 10%;">
  <a style="float: left;width: 60%;text-align: center;">
    <i class="icon icon-m2-outline" style="font-size: 20px;margin-right: 10px;"></i>总收益:$10,0000
  </a>
  <a style="float: left;width: 40%;text-align: left;padding-left: 20px;">
    <i class="icon icon-exchange" style="font-size: 20px;margin-right: 10px; "></i>去交易
  </a>

</div>
<div class="app-divider"></div>
<div class="row" style="border-bottom: #707070 1px solid;">
  <div class="col"><span>日</span></div>
  <div class="col"><span>周</span></div>
  <div class="col"><span>月</span></div>
</div>
<div class="row" style="border-bottom: #707070 1px solid;">
  <div class="col">50%</div>
  <div class="col">59%</div>
  <div class="col">77%</div>
</div>
<div class="row" >
  <div class="col">3名</div>
  <div class="col">5名</div>
  <div class="col">77名</div>
</div>
<div class="app-divider"></div>
<div class="row" style="border-bottom: #707070 1px solid;">
  <div class="col"><span>成员</span></div>
  <div class="col"><span>称号</span></div>
  <div class="col"><span>名次</span></div>
  <div class="col"><span>日盈利率</span></div>
</div>
<div class="row" ng-repeat="n in list">
  <div class="col">
    <img ng-click="showDetail()" src="{{n.img}}" width="30px" height="30px">
    {{n.mName}}
  </div>
  <div class="col">{{n.nickName}}</div>
  <div class="col">{{n.rankNo}}</div>
  <div class="col">{{n.profitandloss}}</div>
</div>




