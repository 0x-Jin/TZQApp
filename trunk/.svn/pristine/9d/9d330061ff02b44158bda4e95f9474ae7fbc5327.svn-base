.app-index-input-search{
  width: 80%;
  height: 25px;
  line-height: 25px;
  padding: 2px;
  top: 15%;
  margin-left: 3%;
  margin-right: 3%;
  background-color: transparent;
  border-color: #fff9ea;
}
.app-index-input-search i{
  margin-right: 5px;
  font-size: 100%;
  color:#222020;
}
.app-index-input-search input{
  color: #222020;
  height: 20px;
}


/**App Team Rank Page**/
.app-rank-summary-type{
  height: 50px;
}
.app-rank-summary-type a{
  width: 50%;
  position: relative;
  float: left;
  height: 50px;
  line-height: 50px;
  text-align: center;
  background: #ffffff;
  font-size: 18px;
  font-weight: bold;
}
.app-rank-summary-type a.active{
  background: #e5e5e5;
}

/**App Team Creation**/
.app-team-creation-view{
  background-color: #f5f5f5;
}
.app-team-creation-body{
  padding:0px 30px 0px 30px;
  overflow: visible;
}

.app-team-creation-body .ruleText{
  border-radius:6px;
  width: 100%;
  height: 80%;
  border: 1px solid #707070;
  overflow-x: hidden;
  overflow-y: auto;
  background-color: #fff;
  margin-bottom: 10px;
  padding: 10px;
}
.app-team-creation-body .checkboxlabel{
  background-color: transparent;
  border: none;
  font-size: inherit;
}

.app-team-creation-body .search-list{

}
.app-team-creation-body .search-list .item{
  padding: 5px 15px;
  height: 60px; line-height: 60px;
}
.app-team-creation-body .search-list .item img,
.app-team-creation-body .search-list .item h2{
  float: left;
}
.app-team-creation-body .search-list .item h2{
  padding: 18px;
}
.app-team-creation-body .search-list .item span{
  float: right;
  color:#fe8308;
  font-size: 12px;
}








