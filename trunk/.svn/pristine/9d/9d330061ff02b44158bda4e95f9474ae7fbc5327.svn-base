/**Customized CSS definition top on the IONIC framework**/
.tabs{
  background-color: #FFFFFF;
  background-image: linear-gradient(0deg, #f2f1f1, #f2f1f1 0%, transparent 0%);
  background-position: center;
  box-shadow:0px 0px 0px 1px #f2f1f1;
  /*background: url("../img/tabs_bg_02.png") no-repeat;*/
  /*background-repeat: no-repeat;*/
  /*background-size:contain;*/
  /*height: 80px;*/
  height: 50px;
  padding-top: 6px;
}

div.tab-nav.tabs a:nth-of-type(3){
  overflow: visible;
  opacity: 1.0;
}

div.tab-nav.tabs a:nth-of-type(3) .tab-title{
  position: relative;
  top:-28px;
}

.tab-item .icon{
  font-size:20px;
  line-height: 1.1;
}

ion-tabs.tabs-color-active-positive .tab-item.tab-item-active,
ion-tabs.tabs-color-active-positive .tab-item.active,
ion-tabs.tabs-color-active-positive .tab-item.activated{
  color:#fcd953;
}

ion-tabs.tabs-color-active-positive .tab-item{
  color:#000;
}

.tabs-icon-top > .tabs .tab-item{
  line-height: 5px;
}


.tab-item .big{
  width: 60px;
  height: 60px;
  /*-webkit-border-radius: 60px;*/

  position: relative;
  top: -33px;
  font-size: 50px;
  line-height: 60px;

  background: url("../img/trade_bg.png") no-repeat;
  /*background-color: #333333;*/
  background-size: 100% auto;
  border:0px;
  color: #fcd953;
}

/**App Common components**/
.clear {
  display: block;
  overflow: hidden;
  clear: both;
  height: 20px;
}
.app-divider {
  background: #f5f5f5;
  display: block;
  overflow: hidden;
  clear: both;
  height: 10px;
}
.app-card-tab-title{
  width: 100%;
  height: 45px;
}

.app-card-tab-title .col{
  float: left;
  width: 33%;
  height: 100%;
  font-size: 16px;
  font-weight: 700;

  text-align: center;
  padding: 10px;
  border-bottom:1px solid #959595;
}
.app-card-tab-title .col:nth-child(3){
  width: 34%;
}
.app-card-tab-title .active{
  border-bottom:2px solid #ffd01b;
  color: #e3b400;
}

.app-common-header{
  height: 45px;
  line-height: 45px;
  padding: 5px;
  background:#fcd953;
}

.app-big-button{
  width: 100%;
  background-color: #f3d356;
  -webkit-border-radius: 5px;
  border: 0px;
  -webkit-box-shadow:0px 3px 0px 0px #b69102 ;
  font-size: 18px;
  font-weight: bold;
}
.btn-white{
  background-color: #fff;
  border: 1px solid #707070;
  font-weight: 400;
  font-size: 20px;
  line-height: 40px;
  -webkit-border-radius: 5px;
}


/**for model**/
.qshow {
  opacity: 0;
}

.qshow.ng-enter-active,
.qshow > .ng-enter-active {
  opacity: 1;
}

.app-popup-top{
  background: url('../img/popup_bg_1.png') repeat-y;
  background-size:0.8% auto;
  height: 100%;
  width: 100%;
  background-position:center 0;
  background-attachment:fixed;
}
.app-popup-content{
  background: url('../img/popup_bg_2.png') no-repeat top;
  background-size:17% auto;
  background-color: #fcd953;
  border: none;
  border-radius: 5px;
  width:100%;
  padding: 50px 20px 0px 20px;
  bottom: -100%;
}

.item:first-child{
  border-top-left-radius: 5px;
  border-top-right-radius: 5px;
}
.item:last-child{
  border-bottom-left-radius: 5px;
  border-bottom-right-radius: 5px;
}


/**App Index**/
.app-index-header{
  height: 45px;
  line-height: 45px;
  padding: 5px;
  background:-webkit-gradient(linear, 0 0, 100% 0, from(#fcd953), to(#e98157));
}


.bar .button.button-icon .icon::before,
.bar .button.button-icon::before,
.bar .button.button-icon.icon-left::before,
.bar .button.button-icon.icon-right::before{
  font-size: 25px;
}

.app-index-input-search input::-webkit-input-placeholder {
  color:#222020;
}

.app-index-slide .slider-pager{
  bottom:0px;
  font-size: 10px;
}
.app-index-myteam{
  margin: 0 auto;
  border-radius: 0px;
  box-shadow: 0px 0px 0px #f5f5f5;
}
.app-index-myteam .app-index-card-divider{
  border: 0px;
}

.team-round-img{
  width: 60px;
  height: 60px;
  -webkit-border-radius: 30px;
}
.app-index-myteam .item{
  padding: 10px 16px 16px 16px;
  font-size: 14px;
}
.app-index-myteam .item .col1{
  float: left;
  width: 25%;
  padding-left: 3%;
}
.app-index-myteam .item .col2{
  float: left;
  width: 50%;
  margin-left: 6%;
}
.app-index-myteam .item .col3{
  float: left;
  width: 15%;
  margin-left: 10px;
  padding-right: 5px;
}
.app-index-myteam .item .col1 .rankno{
  width: 15px;
  height: 15px;
  position: absolute;
  top: 0px;
  left: 0px;
  color: #fffcfd;
  text-align: center;
  font-size: 10px;
}
.app-index-myteam .item .col2 h2{
  font-size: 14px;
  font-weight: 200;
  color: #0c93fb;
}
.app-index-myteam .item .col2 .v{
  color: #e2972d;
  font-size: 15px;
  margin-left: 20px;
}
.app-index-myteam .item .col2 h1{
  font-size: 14px;
  font-weight:bold;
  color: #010813;
  margin: 0 0 3px 0px;

}
.app-index-myteam .item .col2 span{
  color: #010813;
  text-overflow: inherit;
  white-space: normal;
  font-size: 12px;
}
.app-index-myteam .item .col3 span{
  background-color: #56b3fb;
  color: #ffffff;
  padding: 2px 7px 2px 7px;

}
.app-index-myteam .item .col3 h2{
  color: #010813;
  padding: 12px 7px 10px 7px;
  font-size: 16px;
  overflow: visible;
}
.app-index-myteam .item .col3 .fans{
  background-size:contain;
  color: #fe8308;
  font-size: 10px;
  position: relative;
  top: -5px;
}
.app-index-myteam .item .col3 .add{
  background-color: #e2972d;
  font-size: 10px;
  color: #fff;
  text-align: center;
  padding: 5px;
  margin-top: 15px;
}

/**战队首页**/
.app-team-index{

}
.app-team-index .index-header{
  width: 100%;
  height: 190px;
  background-color: #fff;
  overflow: hidden
}

.app-team-index .index-header .toolbar{
  background: transparent; top: 0px;left: 0px;z-index: 50;position: relative;
}
.app-team-index .index-header .team-info{
  text-align: center;
  width: 100%;
  position: relative;
  z-index: 51;
  color: #fff;
}
.app-team-index .index-header .toolbar h1,
.app-team-index .index-header .toolbar a,
.app-team-index .index-header .team-info h5{
  color: #fff;
}
.app-team-index .index-header .bg{
  height: 230px; width:110%;
  overflow: hidden;
  opacity: 0.8;
  -webkit-filter: blur(5px);
  position: absolute;
  top: -10px;
  left: -10px;
  z-index: 48;
}

.app-team-index .app-team-index-tab-hd{
  position: relative;
  background: #fff;
  padding-left: 20%;
  padding-right: 20%;
  padding-bottom: 0px;
  padding-top: 0px;
  text-align: center;

}
.app-team-index .app-team-index-tab-hd .col{
  width: 60px;
  height: 100%;
  font-size: 16px;
  font-weight: 200;
  border: none  ;
  float: left;
}

.app-team-index .app-team-index-tab-hd .active{
  border-bottom:2px solid #ffd01b;
  color: #e3b400;
}
.app-team-index .row{
  margin: 0px;
}
.app-team-index .row .col{
  text-align: center;
}
.app-team-index .row .col span{
  margin-top:3px;
  margin-bottom: 3px;
  font-size: 14px;
  font-weight: 600;
}




