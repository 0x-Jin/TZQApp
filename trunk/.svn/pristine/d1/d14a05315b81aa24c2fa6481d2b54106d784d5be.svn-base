/**Customized CSS definition top on the IONIC framework**/
.tabs{
  background-color: #FFFFFF;
  background-image: linear-gradient(0deg, #f2f1f1, #f2f1f1 0%, transparent 0%);
  background-position: center;
  box-shadow:0px 0px 0px 1px #f2f1f1;
  /*background: url("../img/tabs_bg_02.png") no-repeat;*/
  /*background-repeat: no-repeat;*/
  /*background-size:contain;*/
  /*height: 80px;*/
  height: 50px;
  padding-top: 6px;
}

div.tab-nav.tabs a:nth-of-type(3){
  overflow: visible;
  opacity: 1.0;
}

div.tab-nav.tabs a:nth-of-type(3) .tab-title{
  position: relative;
  top:-28px;
}

.tab-item .icon{
  font-size:20px;
  line-height: 1.1;
}

ion-tabs.tabs-color-active-positive .tab-item.tab-item-active,
ion-tabs.tabs-color-active-positive .tab-item.active,
ion-tabs.tabs-color-active-positive .tab-item.activated{
  color:#fcd953;
}

ion-tabs.tabs-color-active-positive .tab-item{
  color:#000;
}

.tabs-icon-top > .tabs .tab-item{
  line-height: 5px;
}


.tab-item .big{
  width: 60px;
  height: 60px;
  /*-webkit-border-radius: 60px;*/

  position: relative;
  top: -33px;
  font-size: 50px;
  line-height: 60px;

  background: url("../img/trade_bg.png") no-repeat;
  /*background-color: #333333;*/
  background-size: 100% auto;
  border:0px;
  color: #fcd953;
}

/**App Common components**/
.clear {
  display: block;
  overflow: hidden;
  clear: both;
  height: 20px;
}
.app-divider {
  background: #f5f5f5;
  display: block;
  overflow: hidden;
  clear: both;
  height: 8px;
}
.app-card-tab-title{
  width: 100%;
  height: 45px;
}

.app-card-tab-title .col{
  float: left;
  width: 33%;
  height: 100%;
  font-size: 16px;
  font-weight: 700;

  text-align: center;
  padding: 10px;
  border-bottom:1px solid #959595;
}
.app-card-tab-title .col:nth-child(3){
  width: 34%;
}
.app-card-tab-title .active{
  border-bottom:2px solid #ffd01b;
  color: #e3b400;
}

.app-common-header{
  height: 45px;
  line-height: 45px;
  padding: 5px;
  background:#fcd953;
}

.app-big-button{
  width: 100%;
  background-color: #f3d356;
  -webkit-border-radius: 5px;
  border: 0px;
  -webkit-box-shadow:0px 3px 0px 0px #b69102 ;
  font-size: 18px;
  font-weight: bold;
}
.btn-white{
  background-color: #fff;
  border: 1px solid #707070;
  font-weight: 400;
  font-size: 20px;
  line-height: 40px;
  -webkit-border-radius: 5px;
}


/**for model**/
.qshow {
  opacity: 0;
}

.qshow.ng-enter-active,
.qshow > .ng-enter-active {
  opacity: 1;
}

.app-popup-top{
  background: url('../img/popup_bg_1.png') repeat-y;
  background-size:0.8% auto;
  height: 100%;
  width: 100%;
  background-position:center 0;
  background-attachment:fixed;
}
.app-popup-content{
  background: url('../img/popup_bg_2.png') no-repeat top;
  background-size:17% auto;
  background-color: #fcd953;
  border: none;
  border-radius: 5px;
  width:100%;
  padding: 70px 20px 0px 20px;
  bottom: -100%;
}

.item:first-child{
  border-top-left-radius: 5px;
  border-top-right-radius: 5px;
}
.item:last-child{
  border-bottom-left-radius: 5px;
  border-bottom-right-radius: 5px;
}


