/**ICONs definition for APP**/
@font-face {
  font-family: 'icomoon';
  src:url('../fonts/icomoon.eot?7svi8n');
  src:url('../fonts/icomoon.eot?7svi8n#iefix') format('embedded-opentype'),
  url('../fonts/icomoon.ttf?7svi8n') format('truetype'),
  url('../fonts/icomoon.woff?7svi8n') format('woff'),
  url('../fonts/icomoon.svg?7svi8n#icomoon') format('svg');
  font-weight: normal;
  font-style: normal;
}

[class^="icon-"], [class*="icon-"]{
